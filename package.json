{
  "name": "hidenburg",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:flexiooss/hidenburg.git",
  "author": "BrunoFL",
  "devDependencies": {
    "code-altimeter-js": "https://github.com/flexiooss/code-altimeter-js.git",
    "eslint": "^5.7.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-html": "^4.0.6",
    "stylelint": "^9.6.0",
    "stylelint-config-standard": "^18.2.0"
  },
  "dependencies": {
    "@flexio-oss/hotballoon": "^0.1.0-dev",
    "@flexio-oss/atmosphere-layers": "0.0.0-dev",
    "@flexio-oss/global-import-registry": "0.0.0",
    "@flexio-oss/js-type-helpers": "0.0.0",
    "@flexio-oss/js-logger": "0.0.0",
    "@flexio-oss/js-helpers": "0.0.0",
    "@flexio-oss/assert": "0.0.0",
    "flexio-jshelpers": "https://github.com/flexiooss/flexio-jshelpers.git"
  },
  "hotballoon-shed": {
    "build": {
      "builder": "webpack4",
      "entries": [
        "src/main/js/bootstrap.js"
      ],
      "html_template": "src/main/js/index.html",
      "output": "./dist"
    },
    "dev": {
      "server": {
        "host": "ui.flexio.io",
        "proxy": {
          "/api": {
            "target": "https://api.flexio.io",
            "pathRewrite": {
              "^/api": ""
            },
            "secure": true,
            "changeOrigin": true
          }
        }
      }
    },
    "modules": {
      "component-select": "src"
    }
  }
}
